{
  "name": "@australis/tiny-sql",
  "private": true,
  "version": "0.1.0-1",
  "description": "tiny sql mapper for tediousjs",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --env=node",
    "build": "tsc -p .",
    "clean": "rimraf ./lib",
    "rebuild": "npm run clean && npm run build"
  },
  "keywords": [
    "typescript",
    "sql",
    "mapper"
  ],
  "author": "daniel@australis-technica.com",
  "license": "MIT",
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.test.json"
      }
    },
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "devDependencies": {
    "typescript": "2.9.2"
  }
}
